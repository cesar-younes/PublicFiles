{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "right",
            "segments": [
                {
                    "background": "#000000",
                    "foreground": "#ffffff",
                    "style": "plain",
                    "template": " \uf64f {{ .CurrentDate | date .Format }} ",
                    "type": "time"
                },
                {
                    "background": "#3bfc00",
                    "foreground": "#000000",
                    "properties": {
                        "always_enabled": true
                    },
                    "style": "plain",
                    "template": " \ufa1e{{ .FormattedMs }} ",
                    "type": "executiontime"
                }
            ],
            "type": "rprompt"
        },
        {
            "alignment": "left",
            "segments": [
                {
                    "background": "#000000",
                    "foreground": "#3bfc00",
                    "leading_diamond": "\ue0b6",
                    "trailing_diamond": "\ue0b0",
                    "properties": {
                        "folder_icon": "\uf115",
                        "folder_separator_icon": " \ue0b1 ",
                        "max_depth": 2,
                        "style": "agnoster_short"
                    },
                    "style": "diamond",
                    "template": "{{ .Path }}",
                    "type": "path"
                },
                {
                    "background": "#95ffa4",
                    "foreground": "#193549",
                    "powerline_symbol": "\ue0b0",
                    "style": "powerline",
                    "template": "{{ .HEAD }}",
                    "type": "git"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#ffffff",
                    "style": "plain",
                    "template": "> ",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "console_title_template": "📁{{.Folder}}",
    "final_space": true,
    "version": 2
}
